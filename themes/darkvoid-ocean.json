  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#585858"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#b1b1b1"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff7f50"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword.control", "storage.modifier"],
      "settings": {
        "foreground": "#f1f1f1"
      }
    },
    {
      "name": "Type",
      "scope": ["keyword.type", "storage.type"],
      "settings": {
        "foreground": "#a1a1a1"
      }
    },
    {
      "name": "Class, Support",
      "scope": ["entity.name", "support.class"],
      "settings": { "foreground": "#a1a1a1" }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator.arithmetic",
